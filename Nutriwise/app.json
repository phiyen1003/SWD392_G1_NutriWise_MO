{
  "expo": {
    "reactNativeVersion": {
      "newArchEnabled": true
    },
    "name": "NutriWise",
    "slug": "nutriwise",
    "scheme": "nutriwise",
    "version": "1.0.0",
    "platforms": ["ios", "android", "web"], 
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "1f1a1c9d-96a3-4b84-95b8-cfd7232decb5"
      },
      "redirectUri": "https://auth.expo.dev/@quocbaochauu/nutriwise"
    },
    
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.myexpoapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
